generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
url      = env("DATABASE_URL")
}

model Listing {
  id        Int      @id @default(autoincrement())
  name      String
  price     Int
  image     String?
  status    String   @default("Active")
  createdAt DateTime @default(now())
}
